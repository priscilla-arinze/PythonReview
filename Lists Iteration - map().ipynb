   "source": [
    "income = [5, 10, 15, 20]\n",
    "\n",
    "def double_money(dollars):\n",
    "    return dollars * 2\n",
    "\n",
    "doubled_income = list(map(double_money, income))\n",
    "# OR doubled_income = list(map(lambda x: x*2, income))\n",
    "\n",
    "print(doubled_income)"
   ]
