   "source": [
    "ageInput = input(\"Enter your age: \")\n",
    "\n",
    "try:\n",
    "    age = int(ageInput)\n",
    "except:\n",
    "    age = -1\n",
    "\n",
    "if age > 0:\n",
    "    print(\"You're old.\")\n",
    "else:\n",
    "    print(\"Enter a valid age dummy.\")"
